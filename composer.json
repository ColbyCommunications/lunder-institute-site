{
    "name": "colbycommunications/lunder-institute-site",
    "description": "Configuration project for the lunderinstitute.colby.edu site",
    "type": "project",
    "require": {
        "johnpbloch/wordpress-core-installer": "^2.0",
        "johnpbloch/wordpress-core": "6.5.5",
        "wpackagist-plugin/classic-editor": "1.6.3",
        "wpackagist-plugin/capability-manager-enhanced": "2.11.1",
        "wpackagist-plugin/header-footer": "3.2.8",
        "wpackagist-plugin/duplicate-post": "4.5",
        "wpackagist-plugin/wordpress-seo": "23.0",
        "wpackagist-plugin/cloudflare": "4.12.7",
        "wpackagist-plugin/public-post-preview": "2.10.0",
        "wpackagist-plugin/wp-saml-auth": "2.1.4",
        "wpackagist-plugin/siteimprove": "2.0.7",
        "wp-cli/wp-cli-bundle": "^2.4",
        "psy/psysh": "^0.10.4",
        "colbycommunications/advanced-custom-fields-pro": "6.3.3",
        "colbycommunications/gravityforms": "2.8.13",
        "colbycommunications/colby-ticket": "^1.0.1",
        "wpackagist-plugin/wp-search-with-algolia": "2.7.0",
        "vlucas/phpdotenv": "^5.4",
        "wpackagist-plugin/classic-widgets": "0.3",
        "wpackagist-plugin/instagram-feed": "1.12",
        "wpackagist-plugin/popup-maker": "1.19.0",
        "wpackagist-plugin/wp-nested-pages": "3.2.8",
        "wpackagist-plugin/disable-comments": "2.4.6"
    },
    "authors": [
        {
            "name": "Morty Mule",
            "email": "webmaster@colby.edu"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org/"
        },
        {
            "type": "composer",
            "url": "https://satis.colby.edu"
        }
    ],
    "extra": {
        "wordpress-install-dir": "web/wp",
        "installer-paths": {
            "web/wp-content/mu-plugins/{$name}": [
                "type:wordpress-muplugin",
                "wpackagist-plugin/wordpress-seo"
            ],
            "web/wp-content/plugins/{$name}": [
                "type:wordpress-plugin"
            ],
            "web/wp-content/themes/{$name}": [
                "type:wordpress-theme"
            ]
        }
    },
    "config": {
        "allow-plugins": true
    }
}
